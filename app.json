{
  "expo": {
    "name": "CHMA",
    "slug": "CHMA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adeyanju.CHMA"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "splash": {
      "image": "./assets/splashscreen_logo.png",
      "imageWidth": 300,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splashscreen_logo.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "075331ed-f592-4f93-9993-2862473942d6"
      }
    }
  }
}
